<h1>Know you signed up, but cant remember your password?</h1>
    <%= form_with(model: @user) do |form| %>
        <div>
            <%= form.label :email %>
            <%= form.text_field :email %>
        </div>
        <div>
            <%= form.submit "Send secret email with password reset instructions!" %>
        </div>
    <% end %>
</div>