<%= form_with(model: @photo, local:true) do |form| %>
    <%= if @photo.errors.any? do %>
    <div id="error_explanation">
        <h2><%= pluralize(@photo.errors.count, "error") %> 
            prohibited your request from being finalized.
        </h2>

        <ul>
            <%= post.errors.full_messages.each do |message| %>
            <li><%= message %></li>
            <% end %>
        </ul>
    </div>
    <% end %>

    <div>
        <%=  %>
        <%= form.text_field :good_path %>
    </div>

    <div class="field">
        <%= form.label :header_image %>
        <%= form.file_field :header_image %>
    </div>

    <div class="actions">
        <%= form.submit %>
    </div>
<% end %>
<% end %>